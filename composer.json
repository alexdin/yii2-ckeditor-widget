{
    "name": "alexdin/yii2-ckeditor-widget",
    "description": "CKEditor widget for Yii2.",
    "type": "yii2-extension",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "ckeditor"
    ],
    "homepage": "http://yiiwheels.com/extension/ckeditor-widget",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/alexdin/yii2-ckeditor-widget"
    },
    "require": {
        "yiisoft/yii2": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "alexdin/ckeditor-releases": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "alexdin\\ckeditor\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
